tags <- function(m, k, s) {
# base cases
if (m <= 0 | k <= 0 | s <= 0) {
return(0)
} else if (k <= m) {
return(1)
} else if (k > s * m) {
return(0)
}
# recursive cases
else {
p <- 0
for (i in 1:m) {
p <- p + tags(m, k - i, s - 1)
}
return(p/m)
}
}
